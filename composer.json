{
  "name": "simtabi/laratoast",
  "description": "Sweetalert and Toaster notifications for Laravel livewire",
  "type": "library",
  "require": {
    "spatie/laravel-package-tools": "^1.4.3",
    "illuminate/contracts": "^8.51"
  },
  "require-dev": {
    "orchestra/testbench": "^6.18"
  },
  "license": "MIT",
  "authors": [
    {
      "name": "Imani Manyara",
      "email": "imani@simtabi.com",
      "homepage": "https://simtabi.com",
      "role": "Developer"
    }
  ],
  "autoload": {
    "psr-4": {
      "Simtabi\\Laratoast\\": "src"
    },
    "files": [
      "helpers/helpers.php"
    ]
  },
  "extra": {
    "laravel": {
      "providers": [
        "Simtabi\\Laratoast\\Providers\\LaratoastServiceProvider"
      ],
      "aliases": {
        "LaratoastFlash": "Simtabi\\Laratoast\\Facades\\LaratoastFlashFacade",
        "LaratoastHelper": "Simtabi\\Laratoast\\Facades\\LaratoastHelperFacade"
      }
    }
  }
}
